<div id="feed">
{{#each feedItems}}
<div id="review-{{this.id}}" class="feed-review">
{{> review review=this feed=true thingFeed=../thingFeed}}
</div>
{{#unless @last}}
<hr class="feed-separator">
{{/unless}}
{{/each}}
<p>
{{#if paginationURL}}
<a class="pagination-link" href="{{paginationURL}}"><span class="fa fa-angle-double-left spaced-icon"></span>{{{__ "older reviews"}}}</a>
{{/if}}
</div>
